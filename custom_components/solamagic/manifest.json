{
  "domain": "solamagic",
  "name": "Solamagic",
  "version": "0.3.25",
  "documentation": "https://github.com/Pocke01/homeassistant-solamagic",
  "codeowners": ["@Pocke01"],
  "issue_tracker": "https://github.com/Pocke01/homeassistant-solamagic/issues",
  "requirements": ["bleak-retry-connector>=3.5.1"],
  "integration_type": "hub",
  "iot_class": "local_push",
  "dependencies": ["bluetooth"],
  "after_dependencies": ["bluetooth"],
  "bluetooth": [
    {
      "connectable": true,
      "local_name": "D0:65:4C:*"
    },
    {
      "connectable": true,
      "manufacturer_id": 89
    }
  ]
}